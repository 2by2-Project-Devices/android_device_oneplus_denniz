type opluscam_app, domain, coredomain;

app_domain(opluscam_app)
net_domain(opluscam_app)

binder_call(opluscam_app, gpuservice)
binder_call(opluscam_app, update_engine)

allow opluscam_app persist_camera_file:dir rw_dir_perms;
allow opluscam_app persist_data_file:dir r_dir_perms;
allow opluscam_app mnt_vendor_file:dir rw_dir_perms;
allow opluscam_app apusys_device:chr_file rw_file_perms;
allow opluscam_app vendor_file:file rx_file_perms;

get_prop(opluscam_app, vendor_default_prop)
get_prop(opluscam_app, build_prop)
